{
  "name": "Yard",
  "version": "1.2",
  "description": "Yard Yard",
  "permissions": [
    "bookmarks",
    "activeTab",
    "*://*/*"
  ],
  "browser_action": {
      "default_title": "Yard Easy Access",
      "default_icon": "icon128.png",
      "default_popup": "yard.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'  https://rusty.blue; object-src 'self'",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}
